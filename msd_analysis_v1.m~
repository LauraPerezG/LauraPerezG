% Auto correlation analysis for Nexp

% Initialization of the workspace
close all

clear;

%close all;

addpath msd



subs=1; %use a subsampled data set


    
load(['Fig9_1P' num2str(j) '_S.mat']);

Vx=Vx*Sdx;

[k_msd, Ek_msd, D_msd, ED_msd, tau, mmsd, Emsd,indc]=msd_nlfit(Vx(1:subs:end,:),T,dt*subs,1000);

save([nfile filesep datafile '_fit_msd_subs' num2str(subs)  '.mat'],'k_msd', 'Ek_msd', 'D_msd', 'ED_msd', 'tau', 'mmsd', 'Emsd','indc');
 
savePDF(gcf,[nfile filesep datafile '_msd_subs' num2str(subs) '.pdf'])

end
